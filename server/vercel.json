{
    "version": 2,
    "builds": [
        {
            "src": "dist/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/books/highest-copies",
            "dest": "/dist/server.js"
        },
        {
            "src": "/api/books/([a-zA-Z0-9]+)",
            "dest": "/dist/server.js"
        },
        {
            "src": "/api/books",
            "dest": "/dist/server.js"
        }
    ]
}